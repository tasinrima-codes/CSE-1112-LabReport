#include <stdio.h>
#include <string.h>

struct Student {
    char name[50];
    int id;
} s[100], temp;

int main() {
    int n;
    printf("Enter number of students: ");
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
        scanf("%s %d", s[i].name, &s[i].id);
    for (int i = 0; i < n - 1; i++)
        for (int j = i + 1; j < n; j++)
            if (strcmp(s[i].name, s[j].name) > 0) {
                temp = s[i];
                s[i] = s[j];
                s[j] = temp;
            }
    printf("\nSorted List:\n");
    for (int i = 0; i < n; i++)
        printf("%s %d\n", s[i].name, s[i].id);
    return 0;
}
