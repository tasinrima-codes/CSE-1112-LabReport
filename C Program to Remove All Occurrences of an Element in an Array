#include <stdio.h>

int main() {
    int n, x;
    printf("Enter size: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter elements:\n");
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter element to remove: ");
    scanf("%d", &x);

    printf("Array after removing %d:\n", x);
    for (int i = 0; i < n; i++)
        if (arr[i] != x)
            printf("%d ", arr[i]);

    return 0;
}
