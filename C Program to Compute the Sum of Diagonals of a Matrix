#include <stdio.h>

int main() {
    int n, i, j;
    printf("Enter size of square matrix: ");
    scanf("%d", &n);

    int a[n][n];
    int sum1 = 0, sum2 = 0;

    printf("Enter matrix elements:\n");
    for (i = 0; i < n; i++)
        for (j = 0; j < n; j++)
            scanf("%d", &a[i][j]);

    for (i = 0; i < n; i++) {
        sum1 += a[i][i];            
        sum2 += a[i][n - i - 1];    
    }

    printf("Main diagonal sum = %d\n", sum1);
    printf("Secondary diagonal sum = %d\n", sum2);

    return 0;
}
