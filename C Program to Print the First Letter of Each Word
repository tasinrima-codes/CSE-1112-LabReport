#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char str[200];
    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    int i;
    if (!isspace(str[0])) printf("%c", str[0]);

    for (i = 1; str[i]; i++) {
        if (isspace(str[i-1]) && !isspace(str[i]))
            printf("%c", str[i]);
    }
    return 0;
}
