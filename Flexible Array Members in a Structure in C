#include <stdio.h>
#include <stdlib.h>

struct Data {
    int size;
    int arr[];
};

int main() {
    int n;
    printf("Enter size: ");
    scanf("%d", &n);
    struct Data *d = malloc(sizeof(struct Data) + n * sizeof(int));
    d->size = n;
    for (int i = 0; i < n; i++) d->arr[i] = i + 1;
    for (int i = 0; i < n; i++) printf("%d ", d->arr[i]);
    free(d);
    return 0;
}
