#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char str[500];
    int alphabet[26] = {0}, i;
    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    for (i = 0; str[i]; i++) {
        if (isalpha(str[i]))
            alphabet[tolower(str[i]) - 'a'] = 1;
    }

    for (i = 0; i < 26; i++) {
        if (alphabet[i] == 0) {
            printf("Not a pangram");
            return 0;
        }
    }
    printf("Pangram");
    return 0;
}
