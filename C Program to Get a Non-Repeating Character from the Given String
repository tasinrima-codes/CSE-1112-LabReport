#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i, j, len, found;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")] = 0;
    len = strlen(str);

    for (i = 0; i < len; i++) {
        found = 0;
        for (j = 0; j < len; j++) {
            if (i != j && str[i] == str[j]) {
                found = 1;
                break;
            }
        }
        if (!found) {
            printf("First non-repeating character: %c", str[i]);
            return 0;
        }
    }
    printf("No non-repeating character found.");
    return 0;
}
