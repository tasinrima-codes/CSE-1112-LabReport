#include <stdio.h>

int main() {
    int r, c;
    printf("Enter rows and columns: ");
    scanf("%d %d", &r, &c);

    int a[r][c], b[c][r];

    printf("Enter matrix elements:\n");
    for (int i = 0; i < r; i++)
        for (int j = 0; j < c; j++)
            scanf("%d", &a[i][j]);

    // Rotation (90Â° clockwise)
    for (int i = 0; i < r; i++)
        for (int j = 0; j < c; j++)
            b[j][r - 1 - i] = a[i][j];

    printf("Matrix after rotation:\n");
    for (int i = 0; i < c; i++) {
        for (int j = 0; j < r; j++)
            printf("%d ", b[i][j]);
        printf("\n");
    }

    return 0;
}
