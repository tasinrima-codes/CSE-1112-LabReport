#include <stdio.h>

int main() {
    int n, r;
    printf("Enter size: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter elements:\n");
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter number of rotations: ");
    scanf("%d", &r);

    r = r % n;

    int temp[r];

    for (int i = 0; i < r; i++)
        temp[i] = arr[n - r + i];

    for (int i = n - 1; i >= r; i--)
        arr[i] = arr[i - r];

    for (int i = 0; i < r; i++)
        arr[i] = temp[i];

    printf("Array after rotation:\n");
    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
