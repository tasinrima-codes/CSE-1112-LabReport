#include <stdio.h>
#include <string.h>

int main() {
    FILE *f = fopen("data.txt", "r");
    char str[200], pattern[50];
    printf("Enter pattern: ");
    scanf("%s", pattern);

    while (fgets(str, sizeof(str), f)) {
        if (strstr(str, pattern))
            printf("%s", str);
    }
    fclose(f);
    return 0;
}
